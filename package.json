{
  "name": "amber-lib-js",
  "version": "1.0.0",
  "private": true,
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "classnames": "^2.2.5",
    "griddle-react": "^1.9.0",
    "moment": "^2.19.1",
    "react-input-mask": "^1.0.7",
    "react-modal": "^3.0.0",
    "react-select": "^1.0.0-rc.10",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "autoprefixer": "7.1.2",
    "ava": "^0.22.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "browser-env": "^3.2.1",
    "css-loader": "0.28.4",
    "eslint": "4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "ignore-styles": "^5.0.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^15.6.1",
    "react-svg-loader": "^2.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2"
  },
  "description": "A wonderful place for all of our shareable components!",
  "main": "build/index.js",
  "scripts": {
    "start": "start-storybook -p 6006",
    "clean": "rimraf build",
    "build": "babel src --out-dir build --copy-files --source-maps",
    "test": "ava",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmberEngine/amber-lib-js.git"
  },
  "author": "Amber Engine",
  "bugs": {
    "url": "https://github.com/AmberEngine/amber-lib-js/issues"
  },
  "homepage": "https://github.com/AmberEngine/amber-lib-js#readme",
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!node_modules/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "verbose": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "ignore-styles",
      "./config/testHelpers.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
